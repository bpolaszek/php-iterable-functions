{
    "name": "bentools/iterable-functions",
    "description": "Provides functions for iterable variables: is_iterable(), iterable_to_array()",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BenTools\\IterableFunctions\\": "src"
        },
        "files": ["src/iterable-functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\IterableFunctions\\Tests\\": "tests"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2",
        "pestphp/pest": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.67",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "symfony/var-dumper": "^5.2"
    },
    "config": {
        "sort-packages": true
    }
}
